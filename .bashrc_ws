### .bashrc_ws
# vim: set filetype=sh :

# William Stark (william.stark.5000@gmail.com)


### load SSH keys to the authentication agent
#eval `ssh-agent -s` &> /dev/null
[[ -f "$HOME/.ssh/id_rsa_github" ]] && ssh-add "$HOME/.ssh/id_rsa_github" &> /dev/null


# z
# https://github.com/rupa/z
Z_ROOT_DIRECTORY="$HOME/data/programs/z"
[[ -d "$Z_ROOT_DIRECTORY" ]] && export Z_ROOT_DIRECTORY

### Python pyenv
[[ -d "$HOME/.pyenv" ]] && export PYENV_ROOT="$HOME/.pyenv"

### Node.js nvm
[[ -d "$HOME/.nvm" ]] && export NVM_DIR="$HOME/.nvm"

### Java jenv
[[ -d "$HOME/.jenv" ]] && export JENV_ROOT="$HOME/.jenv"
